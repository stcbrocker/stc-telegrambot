from telethon import TelegramClient, events
api_id = 29831238
api_hash = '11211a9254f6d1a13f178047bc6ea29a'
# Ganti dengan file ID kamu
file_id_buy = 'AgACAgUAAxkBAAIBFmiO1To5PPgD9ihDSX0pB7B8fY4aAALcxjEbo1F4VNfkXiosgi_NAQADAgADeAADNgQ'
file_id_sell = 'AgACAgUAAxkBAAIBGGiO1T9xUGsP4N59A62O2-HTJpIgAALdxjEbo1F4VNqWcLs_1Tt1AQADAgADeAADNgQ'
# Channel kamu
channel_sinyal = -2511199734
client = TelegramClient('bot_session', api_id, api_hash)
@client.on(events.NewMessage(chats=channel_sinyal))
async def handler(event):
    message_text = event.raw_text.lower()
    if 'entry buy' in message_text:
        await client.send_file(channel_sinyal, file_id_buy)
        print("âœ… Gambar BUY dikirim")
    elif 'entry sell' in message_text:
        await client.send_file(channel_sinyal, file_id_sell)
        print("âœ… Gambar SELL dikirim")
print("ðŸ¤– Bot gambar aktif... Menunggu sinyal...")
client.start()
client.run_until_disconnected()
